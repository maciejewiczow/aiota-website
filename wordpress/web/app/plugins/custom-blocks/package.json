{
    "name": "custom-blocks",
    "version": "1.0.0",
    "private": true,
    "description": "Custom blocks editor & save functionality for the Gutenberg block editor",
    "scripts": {
        "start": "wp-scripts start",
        "build": "wp-scripts build",
        "format-and-fix": "prettier-eslint --write \"./src/**/*.{css,js,json,ts,tsx}\"",
        "format-check": "prettier-eslint --list-different \"./src/**/*.{css,js,json,ts,tsx}\"",
        "lint": "eslint .",
        "lint-errors-only": "npm run lint -- --quiet",
        "generate:possible-types": "faust generatePossibleTypes",
        "typecheck": "tsc",
        "prestart": "npm run generate:possible-types",
        "prebuild": "npm run generate:possible-types"
    },
    "dependencies": {
        "@apollo/client": "^3.9.3",
        "@wordpress/block-editor": "^12.13.0",
        "@wordpress/blocks": "^12.22.0",
        "@wordpress/components": "^25.16.0",
        "@wordpress/core-data": "^6.27.2",
        "@wordpress/data": "^9.20.0",
        "classnames": "^2.5.1",
        "graphql": "^16.8.1",
        "lodash": "^4.17.21"
    },
    "devDependencies": {
        "@faustwp/cli": "^2.0.0",
        "@types/lodash": "^4.14.202",
        "@types/wordpress__block-editor": "^11.5.7",
        "@types/wordpress__blocks": "^12.5.9",
        "@typescript-eslint/eslint-plugin": "^6.7.2",
        "@typescript-eslint/parser": "^6.7.2",
        "@wordpress/scripts": "^26.16.0",
        "eslint": "^8.50.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-import": "^2.28.1",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-no-autofix": "^1.2.3",
        "eslint-plugin-only-warn": "^1.1.0",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-simple-import-sort": "^10.0.0",
        "eslint-plugin-storybook": "^0.6.14",
        "prettier": "^2.8.8",
        "prettier-eslint": "^16.3.0",
        "prettier-eslint-cli": "^7.1.0",
        "tsconfig-paths-webpack-plugin": "^4.1.0",
        "typescript": "^5.6.3"
    }
}
