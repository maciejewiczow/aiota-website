{
    "private": true,
    "scripts": {
        "build": "next build",
        "dev": "next dev",
        "start": "next start",
        "format-and-fix": "prettier-eslint --write --prettier-ignore --eslint-ignore \"**/*.{css,js,json,ts,tsx}\"",
        "format-check": "prettier-eslint --list-different --prettier-ignore --eslint-ignore \"**/*.{css,js,json,ts,tsx}\"",
        "lint": "eslint . .storybook",
        "lint-errors-only": "npm run lint -- --quiet",
        "typecheck": "tsc",
        "storybook": "storybook dev -p 6006 --no-open",
        "build-storybook": "storybook build",
        "chromatic": "chromatic --exit-zero-on-changes",
        "generate:possible-types": "faust generatePossibleTypes",
        "generate:gql-types": "graphql-codegen",
        "generate": "concurrently -p name \"npm:generate:*\"",
        "predev": "npm run generate",
        "prebuild": "npm run generate"
    },
